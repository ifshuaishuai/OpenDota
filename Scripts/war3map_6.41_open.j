		endif
		if lgH!=null then
			call UnitAddItem(lgH,CreateItem('hlst',0,0))
		endif
	elseif lF3 then
		call UnitAddItem(lgG,CreateItem('tgxp',0,0))
		if lgh!=null then
			call UnitAddItem(lgh,CreateItem('tgxp',0,0))
		endif
		if lgH!=null then
			call UnitAddItem(lgH,CreateItem('tgxp',0,0))
		endif
	endif
	call EnableTrigger(GetTriggeringTrigger())
