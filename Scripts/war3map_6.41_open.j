				call SetUnitInvulnerable(lSW,true)
				call PolledWait(((DistanceBetweenPoints(lSY,lSy)-90.)/575.))
			else
				set lSv=99
			endif
		endif
	endloop
	call RemoveLocation(lSY)
	call RemoveLocation(lSy)
	call RemoveLocation(lnG)
	call DestroyGroup(lSx)
	call DestroyGroup(lSX)
endfunction

