	endif
	call SetInteger("Stats"+I2S(GetPlayerId(lpp)),"NumOfConsumables",lJQ)
	call SetInteger("Stats"+I2S(GetPlayerId(lpp)),"NumOfVeggies",lJr)
	call SetInteger("Stats"+I2S(GetPlayerId(lpp)),"NumOfWards",lJR)
