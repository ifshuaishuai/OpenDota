		call FastFlush(lf5)
		call SetUnitPathing(lfU,true)
		set lEq=0
		loop
			exitwhen lEq>5
			if IsItemDrops(lfU,lEq)then
				call SetItemDropOnDeath(UnitItemInSlot(lfU,lEq),true)
				if GetItemTypeId(UnitItemInSlot(lfU,lEq))=='oflg'then
