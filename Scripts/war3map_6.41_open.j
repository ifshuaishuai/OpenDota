function HexChar2Digit takes string pChar returns integer
	if pChar=="a"then
		return 10
	elseif pChar=="b"then
		return 11
	elseif pChar=="c"then
		return 12
	elseif pChar=="d"then
		return 13
	elseif pChar=="e"then
		return 14
	elseif pChar=="f"then
		return 15
	else
		return S2I(pChar)
	endif
endfunction

function HexString2Digit takes string pString returns integer
	return HexChar2Digit(SubString(pString,0,1))*16+HexChar2Digit(SubString(pString,1,2))
