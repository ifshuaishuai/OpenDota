				if GetItemTypeId(UnitItemInSlot(lfU,lEq))=='oflg'then
					call SetItemDroppable(UnitItemInSlot(lfU,lEq),false)
				endif
			endif
		endif
		set lEq=lEq+1
	endloop
	set lOZ=lOZ+.2
	call SetReal(lf5,"Duration",lOZ)
	if GetUnitAbilityLevel(lfU,'B047')==0 then
