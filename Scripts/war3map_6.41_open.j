	call TriggerAddCondition(gg_trg_InitSpellsForHero,Condition(function Trig_InitSpellsForHero_Conditions))
	call TriggerAddAction(gg_trg_InitSpellsForHero,function Trig_InitSpellsForHero_Actions)
